SLACK_BOT_PORT=8080
SLACK_BOT_HOST=0.0.0.0

SLACK_BOT_TOKEN=<your slack bot token>
SLACK_SIGNING_SECRET=<your slack bot signing secret>

BITBUCKET_READ_API_TOKEN=<your bitbucket token for read operations>
